    },
    {
      "index": 55,
      "start_time": 218100.0,
      "end_time": 220400.0,
      "text": "\u00a1Gol! \u00a1Gol! \u00a1Gol!"
    },
    {
      "index": 56,
      "start_time": 221000.0,
      "end_time": 224400.0,
