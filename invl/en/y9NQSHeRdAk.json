{
  "video_id": "y9NQSHeRdAk",
  "title": "Ellen Meets Inspiring Woman Who Helped Homeless During Polar Vortex",
  "es": 0,
  "json": [
    {
      "index": 1,
      "start_time": 330.0,
      "end_time": 1860.0,
      "text": "Last week, the polar vortex caused"
    },
    {
      "index": 2,
      "start_time": 1860.0,
      "end_time": 3960.0,
      "text": "some of the coldest temperatures in US history,"
    },
    {
      "index": 3,
      "start_time": 3960.0,
      "end_time": 7800.0,
      "text": "making the city of Chicago colder than Antarctica."
    },
    {
      "index": 4,
      "start_time": 7800.0,
      "end_time": 9300.0,
      "text": "Our next guest used money out of her own pocket"
    },
    {
      "index": 5,
      "start_time": 9300.0,
      "end_time": 12180.0,
      "text": "to buy hotel rooms for more than 100 homeless people"
    },
    {
      "index": 6,
