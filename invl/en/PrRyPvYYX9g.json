    {
      "index": 105,
      "start_time": 243334.0,
      "end_time": 244367.0,
      "text": "in the Russia investigation."
    },
    {
      "index": 106,
      "start_time": 244367.0,
      "end_time": 246399.0,
      "text": "Trump campaign&#39;s chairman Paul Manafort"
    },
    {
      "index": 107,
      "start_time": 246400.0,
      "end_time": 249000.0,
      "text": "shared political polling data with a business associate"
