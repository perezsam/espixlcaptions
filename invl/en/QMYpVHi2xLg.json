    {
      "index": 71,
      "start_time": 140760.0,
      "end_time": 141420.0,
      "text": "Yeah, they did."
    },
    {
      "index": 72,
      "start_time": 141420.0,
      "end_time": 142709.0,
      "text": "They sent it to me to give to you."
    },
    {
      "index": 73,
      "start_time": 142710.0,
      "end_time": 143200.0,
      "text": "It&#39;s here."
    },
    {
      "index": 74,
      "start_time": 143200.0,
      "end_time": 143520.0,
      "text": "Yeah."
    },
    {
      "index": 75,
      "start_time": 143520.0,
      "end_time": 143640.0,
      "text": "Yeah."
    },
    {
      "index": 76,
      "start_time": 143640.0,
      "end_time": 144790.0,
      "text": "Is it?"
    },
    {
      "index": 77,
      "start_time": 144800.0,
      "end_time": 144580.0,
      "text": "Yeah."
    },
    {
      "index": 78,
      "start_time": 148226.0,
      "end_time": 148726.0,
      "text": "Thank you."
    },
    {
      "index": 79,
      "start_time": 148726.0,
      "end_time": 149714.0,
