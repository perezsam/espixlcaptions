    {
      "index": 21,
      "start_time": 43320.0,
      "end_time": 43570.0,
      "text": "Yeah."
    },
    {
      "index": 22,
      "start_time": 43570.0,
      "end_time": 44690.0,
      "text": "Right there."
    },
    {
      "index": 23,
      "start_time": 44690.0,
