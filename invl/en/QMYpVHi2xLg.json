    {
      "index": 32,
      "start_time": 51750.0,
      "end_time": 52250.0,
      "text": "No."
    },
    {
      "index": 33,
      "start_time": 52250.0,
      "end_time": 53190.0,
      "text": "Yeah."
    },
    {
      "index": 34,
      "start_time": 53190.0,
      "end_time": 54220.0,
      "text": "Is that true?"
    },
    {
      "index": 35,
      "start_time": 54220.0,
