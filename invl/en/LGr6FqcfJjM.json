    {
      "index": 119,
      "start_time": 188240.0,
      "end_time": 189230.0,
      "text": "It's just me."
    },
    {
      "index": 120,
      "start_time": 189230.0,
      "end_time": 189989.0,
      "text": "I'm clingy."
