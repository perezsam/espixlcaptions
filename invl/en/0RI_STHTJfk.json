    {
      "index": 129,
      "start_time": 248410.0,
      "end_time": 249870.0,
      "text": "I see."
    },
    {
      "index": 130,
      "start_time": 249870.0,
      "end_time": 251200.0,
      "text": "Had you watched my show--"
    },
    {
      "index": 131,
      "start_time": 251200.0,
