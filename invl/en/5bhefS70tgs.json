    {
      "index": 25,
      "start_time": 57170.0,
      "end_time": 60463.0,
      "text": "My grandpa was very, very racist."
    },
    {
      "index": 26,
      "start_time": 60463.0,
      "end_time": 62435.0,
      "text": "[INSTRUMENTAL MUSIC PLAYING]"
    },
    {
      "index": 27,
      "start_time": 63421.0,
      "end_time": 64650.0,
      "text": "I&#39;m Samantha Bee."
    },
    {
      "index": 28,
      "start_time": 64650.00000000001,
