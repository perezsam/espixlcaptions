    {
      "index": 99,
      "start_time": 220000.0,
      "end_time": 221900.0,
      "text": "Thank you for all your service"
    },
    {
      "index": 100,
      "start_time": 221900.0,
      "end_time": 224760.0,
      "text": "If it takes fighting a war for us to meet,\nit will have been worth it ;))"
    },
    {
      "index": 101,
      "start_time": 225320.0,
      "end_time": 227360.0,
      "text": "I’ll leave you to it"
