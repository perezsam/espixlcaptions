    {
      "index": 102,
      "start_time": 227560.0,
      "end_time": 228080.0,
      "text": "Number three!"
    },
    {
      "index": 103,
      "start_time": 228080.0,
      "end_time": 231280.0,
      "text": "I know my sister like I know my own mind!"
    },
    {
      "index": 104,
      "start_time": 231280.0,
      "end_time": 235480.0,
