    {
      "index": 127,
      "start_time": 274902.0,
      "end_time": 277586.0,
      "text": "So besides freshmen, sophomore, junior, senior"
    },
    {
      "index": 128,
      "start_time": 277586.0,
      "end_time": 279605.0,
      "text": "There aren't any other expressions"
