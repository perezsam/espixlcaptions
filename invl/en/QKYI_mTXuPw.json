    {
      "index": 289,
      "start_time": 739160.0,
      "end_time": 740220.0,
      "text": "I&#39;ve never had this before."
    },
    {
      "index": 290,
      "start_time": 740220.0,
      "end_time": 741940.0,
      "text": "Does it taste good."
