    {
      "index": 140,
      "start_time": 329940.0,
      "end_time": 332520.0,
      "text": "it so many times I&#39;m broken."
    },
    {
      "index": 141,
      "start_time": 332520.0,
      "end_time": 333990.0,
      "text": "Please, give me a Pulitzer."
    },
    {
      "index": 142,
      "start_time": 333990.0,
