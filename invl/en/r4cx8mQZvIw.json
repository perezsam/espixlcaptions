    },
    {
      "index": 78,
      "start_time": 218640.0,
      "end_time": 220640.0,
      "text": "Alright, stop it now."
    },
    {
      "index": 79,
      "start_time": 220640.0,
      "end_time": 222940.0,
      "text": "You’re making everyone ill!"
    },
    {
      "index": 80,
      "start_time": 222940.0,
