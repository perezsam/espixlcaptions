    {
      "index": 86,
      "start_time": 167830.0,
      "end_time": 168950.0,
      "text": "I&#39;m alive and well!"
    },
    {
      "index": 87,
      "start_time": 168950.0,
      "end_time": 173129.0,
      "text": "Haven&#39;t....you know, even though the streets....it&#39;s been hard, it hadn&#39;t brought me down."
    },
    {
      "index": 88,
      "start_time": 173129.0,
      "end_time": 174940.0,
      "text": "I&#39;m alive and still have hope!"
    }
  ]
