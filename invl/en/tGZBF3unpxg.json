    {
      "index": 49,
      "start_time": 124449.0,
      "end_time": 127708.0,
      "text": "\"about how babies are made?\", for example."
    },
    {
      "index": 50,
      "start_time": 127708.0,
      "end_time": 129142.0,
      "text": "It's an important one to know"
    },
    {
      "index": 51,
      "start_time": 129142.0,
      "end_time": 131809.0,
      "text": "to avoid any awkward situations."
    },
    {
      "index": 52,
      "start_time": 133154.0,
      "end_time": 134608.0,
      "text": "I don't want somebody to ask you"
