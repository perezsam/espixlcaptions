    {
      "index": 96,
      "start_time": 266765.0,
      "end_time": 268734.0,
      "text": "IT WAS RIDICULOUSLY EASY."
    },
    {
      "index": 97,
      "start_time": 268767.0,
      "end_time": 270836.0,
      "text": "LIKE, WHAT WAS EVEN GOING ON BACK THEN?"
    },
    {
      "index": 98,
      "start_time": 270869.0,
      "end_time": 274573.0,
      "text": "WHAT WAS A MURDER INVESTIGATION LIKE IN 1935?"
    },
    {
      "index": 99,
      "start_time": 274606.0,
      "end_time": 278440.0,
      "text": "ONE COP WOULD JUST WALK IN AND BE LIKE, \"DETECTIVE,"
