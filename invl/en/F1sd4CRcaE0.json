    {
      "index": 35,
      "start_time": 77843.0,
      "end_time": 79912.0,
      "text": "A SHOW THAT I LOVE."
    },
    {
      "index": 36,
      "start_time": 79945.0,
      "end_time": 81881.0,
      "text": "BECAUSE ON THAT SHOW, YOU CAN SAY THE GROSSEST THINGS"
