    {
      "index": 40,
      "start_time": 88787.0,
      "end_time": 90957.0,
      "text": "BUT PEOPLE WALK AROUND ON SVU  GOING LIKE,"
    },
    {
      "index": 41,
      "start_time": 90990.0,
      "end_time": 95360.0,
      "text": "\"LOOKS LIKE THE VICTIM HAD ANAL CONTUSIONS."
    },
    {
      "index": 42,
      "start_time": 95394.0,
      "end_time": 97130.0,
      "text": "\"YO, LOOKS LIKE WE FOUND SEMEN AND FECAL MATTER"
    },
    {
      "index": 43,
      "start_time": 97163.0,
      "end_time": 99899.0,
      "text": "IN THE VICTIM&#39;S EAR CANAL.\""
    },
    {
      "index": 44,
      "start_time": 99932.0,
      "end_time": 102635.0,
      "text": "THOSE ARE TWO REAL THINGS THAT I HEARD"
    },
    {
      "index": 45,
      "start_time": 102668.0,
