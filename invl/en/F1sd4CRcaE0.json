    {
      "index": 29,
      "start_time": 68334.0,
      "end_time": 69702.0,
      "text": "IT&#39;S RIDICULOUS."
    },
    {
      "index": 30,
      "start_time": 69735.0,
      "end_time": 71370.0,
      "text": "YOU CAN SAY ANYTHING YOU WANT."
    },
    {
      "index": 31,
      "start_time": 71700.0,
      "end_time": 72904.0,
      "text": "AND IF YOU DON&#39;T BELIEVE ME,"
    },
    {
      "index": 32,
      "start_time": 72938.0,
      "end_time": 74740.0,
      "text": "YOU SHOULD WATCH A LITTLE PROGRAM CALLED"
    },
    {
      "index": 33,
      "start_time": 74773.0,
      "end_time": 76109.0,
