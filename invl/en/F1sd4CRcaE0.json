    {
      "index": 106,
      "start_time": 294460.0,
      "end_time": 296762.0,
      "text": "\"I&#39;LL TELL YOU WHAT WE&#39;LL DO."
    },
    {
      "index": 107,
      "start_time": 296795.0,
      "end_time": 299432.0,
      "text": "\"WE&#39;LL DRAW CHALK AROUND WHERE THE BODY IS."
