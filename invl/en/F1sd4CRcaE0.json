  "es": 0,
  "json": [
    {
      "index": 1,
      "start_time": 1667.0,
      "end_time": 3690.0,
      "text": "IT&#39;S WRONG TO MAKE FUN OF PEOPLE, YOU KNOW,"
    },
    {
      "index": 2,
      "start_time": 3102.0,
      "end_time": 4804.0,
      "text": "BUT IT&#39;S SO FUN SOMETIMES."
