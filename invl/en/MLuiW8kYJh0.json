    {
      "index": 153,
      "start_time": 293960.0,
      "end_time": 295561.0,
      "text": "Also, during my freshman year"
    },
    {
      "index": 154,
      "start_time": 295561.0,
      "end_time": 297630.0,
      "text": "I skipped a few of my    general education classes"
