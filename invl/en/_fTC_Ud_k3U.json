    {
      "index": 95,
      "start_time": 209090.0,
      "end_time": 210920.0,
      "text": "Let&#39;s go get Smarter Every Day."
    },
    {
      "index": 96,
      "start_time": 210920.0,
      "end_time": 213080.0,
      "text": "This is Cameron Prince. He runs the number one"
