    },
    {
      "index": 82,
      "start_time": 269620.0,
      "end_time": 270680.0,
      "text": "Free-for-all!"
    },
    {
      "index": 83,
      "start_time": 270880.0,
      "end_time": 271420.0,
      "text": "GO!"
    },
    {
      "index": 84,
      "start_time": 354500.0,
      "end_time": 355400.0,
      "text": "What are you doing?"
    },
    {
      "index": 85,
      "start_time": 355400.0,
      "end_time": 357900.0,
      "text": "Dude! This Nintendoe Paper is freaking awesome man!"
    },
    {
      "index": 86,
      "start_time": 358000.0,
      "end_time": 360980.0,
      "text": "Aw, if you like that, you're gonna love Microsoft's new system."
    },
    {
      "index": 87,
      "start_time": 361100.0,
      "end_time": 361880.0,
      "text": "What's that?"
    },
    {
      "index": 88,
