{
  "video_id": "MteA2zx9g7k",
  "title": "2017 USTA/ITF Junior Camp of the Americas",
  "json": [
    {
      "index": 1,
      "start_time": 400.0,
      "end_time": 540.0,
      "text": "Jason Harnett,"
    },
    {
      "index": 2,
      "start_time": 540.0,
      "end_time": 2560.0,
      "text": "Here at the Lakeshore Foundation for the"
    },
    {
      "index": 3,
