    {
      "index": 81,
      "start_time": 283221.0,
      "end_time": 285194.0,
      "text": "Mitchell, this is Ethan Zuckerman."
    },
    {
      "index": 82,
      "start_time": 285194.0,
      "end_time": 288005.0,
      "text": "He's the director of the MIT Center for Civic Media."
