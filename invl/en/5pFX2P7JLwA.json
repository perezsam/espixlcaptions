    {
      "index": 77,
      "start_time": 266536.0,
      "end_time": 268688.0,
      "text": "Why? HOW?"
    },
    {
      "index": 78,
      "start_time": 276062.0,
      "end_time": 278121.0,
      "text": "Hello Mitchell. I've been waiting for you."
