    {
      "index": 52,
      "start_time": 152040.0,
      "end_time": 154040.0,
      "text": "the Saint Lawrence River"
    },
    {
      "index": 53,
      "start_time": 154040.0,
      "end_time": 156040.0,
      "text": "became a massive artery"
    },
    {
      "index": 54,
      "start_time": 156040.0,
      "end_time": 158040.0,
      "text": "in this huge trading network that stretched"
    },
    {
      "index": 55,
      "start_time": 158040.0,
      "end_time": 160040.0,
      "text": "all the way to the Great Lakes"
    },
    {
      "index": 56,
      "start_time": 160040.0,
      "end_time": 162040.0,
      "text": "based on fur trading with the local tribes."
