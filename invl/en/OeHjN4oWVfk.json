    {
      "index": 4,
      "start_time": 8800.0,
      "end_time": 10900.0,
      "text": "Remember stupid ass Dark Master?"
    },
    {
      "index": 5,
      "start_time": 10900.0,
      "end_time": 13660.0,
      "text": "I must have been like 12 years old when I got my Xbox"
    },
    {
      "index": 6,
      "start_time": 13660.0,
      "end_time": 15660.0,
      "text": "I got Return to Castle Wolfenstein"
