    {
      "index": 32,
      "start_time": 110360.0,
      "end_time": 116640.0,
      "text": "-tion."
    },
    {
      "index": 33,
      "start_time": 117320.0,
      "end_time": 119320.0,
      "text": "It's really, really fast."
    },
    {
      "index": 34,
      "start_time": 119400.0,
      "end_time": 122160.0,
      "text": "In fact, often the schwa sound"
