    {
      "index": 114,
      "start_time": 227273.0,
      "end_time": 228716.0,
      "text": "Let’s open..."
    },
    {
      "index": 115,
      "start_time": 228716.0,
      "end_time": 232443.0,
      "text": "Today we have a pot with a separator in between"
