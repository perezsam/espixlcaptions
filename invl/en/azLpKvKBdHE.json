    },
    {
      "index": 69,
      "start_time": 148060.0,
      "end_time": 149120.0,
      "text": "and then like, pass away?"
    },
    {
      "index": 70,
      "start_time": 149120.0,
      "end_time": 149760.0,
      "text": "Pass away"
    },
    {
      "index": 71,
      "start_time": 149760.0,
      "end_time": 150860.0,
      "text": "Like that"
    },
    {
      "index": 72,
      "start_time": 150860.0,
