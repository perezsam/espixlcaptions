    {
      "index": 50,
      "start_time": 115485.0,
      "end_time": 116949.0,
      "text": "How about you?"
    },
    {
      "index": 51,
      "start_time": 116949.0,
      "end_time": 118032.0,
      "text": "- Oh, well I."
