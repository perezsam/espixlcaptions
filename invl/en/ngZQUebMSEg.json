    {
      "index": 46,
      "start_time": 106886.0,
      "end_time": 108808.0,
      "text": "Let us out of here!"
    },
    {
      "index": 47,
      "start_time": 108808.0,
      "end_time": 110281.0,
      "text": "- Well, hang on Louise."
