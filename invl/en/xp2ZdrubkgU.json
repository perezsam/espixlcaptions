    {
      "index": 160,
      "start_time": 419479.0,
      "end_time": 420479.0,
      "text": "AS SOON AS THEY STARTED THIS."
    },
    {
      "index": 161,
      "start_time": 420479.0,
      "end_time": 421479.0,
      "text": "IF HE IS LOOKING TO RECRUIT BETTER,"
    },
    {
      "index": 162,
      "start_time": 421479.0,
      "end_time": 422479.0,
      "text": "MISSION ACCOMPLISHED."
    },
    {
      "index": 163,
      "start_time": 422479.0,
      "end_time": 423479.0,
      "text": "REALLY NICE PR MOVE AND WORTH THE EXTRA"
    },
    {
      "index": 164,
      "start_time": 423479.0,
      "end_time": 424479.0,
      "text": "$10,000."
    },
    {
      "index": 165,
      "start_time": 424479.0,
