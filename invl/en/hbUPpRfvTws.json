    {
      "index": 20,
      "start_time": 54590.0,
      "end_time": 56030.0,
      "text": "Not overly panic?"
    },
    {
      "index": 21,
      "start_time": 56030.0,
      "end_time": 59170.0,
      "text": "What’s there to not-overly panic about?"
    },
    {
      "index": 22,
      "start_time": 59170.0,
      "end_time": 60510.0,
      "text": "Military jets and navy ships"
    },
    {
      "index": 23,
      "start_time": 60510.0,
      "end_time": 63230.0,
      "text": "scrambled off the coast of Taiwan on Wednesday."
    },
    {
      "index": 24,
      "start_time": 63230.0,
      "end_time": 66170.0,
      "text": "The latest sign of heightened tensions across the water"
    },
    {
      "index": 25,
      "start_time": 66170.0,
      "end_time": 67990.0,
      "text": "as a group of Chinese warships"
    },
    {
      "index": 26,
      "start_time": 67990.0,
      "end_time": 70230.0,
      "text": "sailed too close for comfort."
