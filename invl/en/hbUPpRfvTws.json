    {
      "index": 41,
      "start_time": 115409.0,
      "end_time": 116409.0,
      "text": "Earlier this week,"
    },
    {
      "index": 42,
      "start_time": 116409.0,
      "end_time": 118240.0,
      "text": "China sent eight warplanes over the waters"
    },
    {
      "index": 43,
      "start_time": 118240.0,
      "end_time": 120930.0,
      "text": "between Japan and South Korea."
