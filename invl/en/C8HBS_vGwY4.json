    {
      "index": 140,
      "start_time": 276300.0,
      "end_time": 278640.0,
      "text": "0-0-0."
    },
    {
      "index": 141,
      "start_time": 278640.0,
      "end_time": 280500.0,
      "text": "Oh my-- 0."
    },
    {
      "index": 142,
      "start_time": 280500.0,
      "end_time": 281740.0,
      "text": "Yeah, that adds up to zero."
    },
    {
      "index": 143,
      "start_time": 285415.0,
      "end_time": 287130.0,
      "text": "Let me see this building, bro."
