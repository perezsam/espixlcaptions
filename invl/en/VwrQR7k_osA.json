  "json": [
    {
      "index": 1,
      "start_time": 9480.0,
      "end_time": 13120.0,
      "text": "Well hello! I'm Emma from mmmEnglish!"
    },
    {
      "index": 2,
      "start_time": 13840.0,
      "end_time": 17680.0,
      "text": "Now I have to admit that I'm a huge fan of adjectives."
