      "index": 113,
      "start_time": 337700.0,
      "end_time": 339839.0,
      "text": "THEY'RE VICIOUS."
    },
    {
      "index": 114,
      "start_time": 339839.0,
      "end_time": 343209.0,
      "text": "I DON'T KNOW WHAT THIS DOES TO A YOUNG KIDS' MINDS."
    },
    {
      "index": 115,
      "start_time": 343209.0,
      "end_time": 345345.0,
      "text": "THE INTERNET, MOVIES, YOU LOOK AT THESE MOVIES THAT ARE AROUND"
