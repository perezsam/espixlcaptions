    {
      "index": 116,
      "start_time": 345345.0,
      "end_time": 346245.0,
      "text": "TODAY."
    },
    {
      "index": 117,
      "start_time": 346245.0,
      "end_time": 349349.0,
      "text": "I SEE JUST BY A COMMERCIAL THE LEVEL OF CRAZINESS AND"
