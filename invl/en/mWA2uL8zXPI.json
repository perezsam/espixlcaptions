    },
    {
      "index": 18,
      "start_time": 65540.0,
      "end_time": 66756.0,
      "text": "Come on, ladies."
    },
    {
      "index": 19,
      "start_time": 66780.0,
      "end_time": 68676.0,
      "text": "(Applause)"
    },
    {
      "index": 20,
      "start_time": 68700.0,
      "end_time": 72516.0,
      "text": "I spent three years talking to girls ages 15 to 20"
    },
    {
      "index": 21,
      "start_time": 72540.0,
