    {
      "index": 26,
      "start_time": 87220.0,
      "end_time": 88436.0,
      "text": "who told me,"
    },
    {
      "index": 27,
      "start_time": 88460.0,
      "end_time": 91716.0,
      "text": "\"I come from a long line of smart, strong women."
    },
    {
      "index": 28,
      "start_time": 91740.0,
      "end_time": 93955.0,
      "text": "My grandmother was a firecracker,"
    },
    {
      "index": 29,
      "start_time": 93980.0,
      "end_time": 95836.0,
      "text": "my mom is a professional,"
    },
    {
      "index": 30,
      "start_time": 95860.0,
      "end_time": 99220.0,
