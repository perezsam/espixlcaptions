{
  "video_id": "UZ0ausMICgM",
  "title": "CLEAREST WATER on Planet Earth?",
  "json": [
    {
      "index": 1,
      "start_time": 867.0,
      "end_time": 3236.0,
      "text": "- [Mark] Ahh, was that graceful?"
    },
    {
      "index": 2,
      "start_time": 3236.0,
      "end_time": 5805.0,
