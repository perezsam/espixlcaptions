    },
    {
      "index": 112,
      "start_time": 392100.0,
      "end_time": 393810.0,
      "text": "One chili and one garlic. Rock, paper, scissors."
    },
    {
      "index": 113,
      "start_time": 393894.0,
      "end_time": 395145.0,
      "text": "Paper! Yeah!"
    },
    {
      "index": 114,
      "start_time": 395229.0,
      "end_time": 396522.0,
      "text": "You really can&#39;t."
    },
    {
      "index": 115,
      "start_time": 396605.0,
      "end_time": 398565.0,
