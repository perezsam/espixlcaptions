    {
      "index": 97,
      "start_time": 352311.0,
      "end_time": 354897.0,
      "text": "I&#39;m the big brother of BTS and my name is Jin."
    },
    {
      "index": 98,
      "start_time": 354980.0,
      "end_time": 356815.0,
      "text": "I&#39;m in charge of having the amazingly handsome face."
    },
    {
      "index": 99,
      "start_time": 360194.0,
      "end_time": 364698.0,
      "text": "Are you, by any chance, a celebrity? You&#39;re so handsome."
