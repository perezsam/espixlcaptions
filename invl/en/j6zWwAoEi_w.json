    {
      "index": 100,
      "start_time": 364781.0,
      "end_time": 365991.0,
      "text": "Needless to say."
    },
    {
      "index": 101,
      "start_time": 366740.0,
      "end_time": 368535.0,
      "text": "I&#39;m J-Hope of BTS."
    },
    {
      "index": 102,
      "start_time": 368619.0,
      "end_time": 371496.0,
      "text": "I&#39;m like the person that connects the younger and older members."
    },
    {
      "index": 103,
      "start_time": 371580.0,
      "end_time": 373916.0,
      "text": "It&#39;s just a matter of closing the distance."
