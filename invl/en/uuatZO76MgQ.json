    {
      "index": 127,
      "start_time": 418273.0,
      "end_time": 420352.0,
      "text": "But this got me to thinking."
    },
    {
      "index": 128,
      "start_time": 420376.0,
      "end_time": 422450.0,
      "text": "Nobody deserves this."
    },
    {
      "index": 129,
      "start_time": 422474.0,
