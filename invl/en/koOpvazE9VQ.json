    {
      "index": 91,
      "start_time": 348614.0,
      "end_time": 351724.0,
      "text": "She didn&#39;t grow up on the streets, in society."
    },
    {
      "index": 92,
      "start_time": 351724.0,
      "end_time": 354920.0,
      "text": "No. She grew up in prison."
    },
    {
      "index": 93,
      "start_time": 357380.0,
