    {
      "index": 16,
      "start_time": 58629.0,
      "end_time": 60850.0,
      "text": "One magical night,"
    },
    {
      "index": 17,
      "start_time": 61165.0,
      "end_time": 64525.0,
      "text": "her daddy did things to her that he liked."
    },
    {
      "index": 18,
      "start_time": 65575.0,
      "end_time": 67810.0,
      "text": "Oh, but she knew"
    },
    {
      "index": 19,
      "start_time": 67810.0,
      "end_time": 71387.0,
      "text": "♪ oh, she knew ♪"
    },
    {
      "index": 20,
      "start_time": 71387.0,
      "end_time": 73109.0,
      "text": "She knew it wasn&#39;t right."
    },
    {
      "index": 21,
      "start_time": 74599.0,
      "end_time": 75815.0,
      "text": "That was a storm."
    },
    {
      "index": 22,
      "start_time": 77235.0,
      "end_time": 80605.0,
      "text": "♪ The storm ♪"
    },
    {
      "index": 23,
      "start_time": 81155.0,
      "end_time": 85565.0,
