      "start_time": 334858.0,
      "end_time": 336627.0,
      "text": "Buddy: UP EASY. EVEN. EVEN. EVEN. EVEN."
    },
    {
      "index": 132,
      "start_time": 336627.0,
      "end_time": 337924.0,
      "text": "KEEP IT EVEN."
    },
    {
      "index": 133,
      "start_time": 337924.0,
      "end_time": 339726.0,
      "text": "KEEP SPINNING. EVEN. EVEN. EVEN."
    },
    {
      "index": 134,
      "start_time": 339726.0,
      "end_time": 340759.0,
      "text": "WHOA! WHOA!"
    },
    {
      "index": 135,
      "start_time": 340759.0,
      "end_time": 342231.0,
      "text": "OKAY. RIGHT THERE. RIGHT THERE."
    },
    {
      "index": 136,
      "start_time": 342231.0,
      "end_time": 343792.0,
      "text": "OKAY, NOW, JOEY, UNDER."
    },
    {
      "index": 137,
      "start_time": 343792.0,
      "end_time": 347198.0,
      "text": "LIFT IT UP A LITTLE BIT. OKAY."
    },
    {
      "index": 138,
      "start_time": 347198.0,
      "end_time": 350132.0,
      "text": "EASY. EASY. EASY. EASY."
    },
    {
      "index": 139,
      "start_time": 350132.0,
      "end_time": 352759.0,
      "text": "EASY. EASY. EASY. EASY. OKAY. CLOSER."
    },
    {
