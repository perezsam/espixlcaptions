    {
      "index": 141,
      "start_time": 354693.0,
      "end_time": 356231.0,
      "text": "OH. OH. NO, I GOT IT, JOE."
    },
    {
      "index": 142,
      "start_time": 356231.0,
      "end_time": 357924.0,
      "text": "OH. OH."
    },
    {
      "index": 143,
      "start_time": 357924.0,
      "end_time": 359429.0,
      "text": "RIGHT THERE. RIGHT THERE. OKAY."
    },
    {
      "index": 144,
      "start_time": 359429.0,
      "end_time": 361198.0,
      "text": "THAT&#39;S IT. THAT&#39;S WHERE SHE GOES."
