    {
      "index": 53,
      "start_time": 124000.0,
      "end_time": 125166.0,
      "text": "WHAT THE RISK POOL LOOKS LIKE,"
    },
    {
      "index": 54,
      "start_time": 125166.0,
      "end_time": 127100.0,
      "text": "HOW MANY ELDERLY PEOPLE ARE--"
    },
    {
      "index": 55,
      "start_time": 127100.0,
      "end_time": 128400.0,
      "text": "HOW MANY HEALTHY PEOPLE,"
    },
    {
      "index": 56,
      "start_time": 128400.0,
