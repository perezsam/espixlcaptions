    {
      "index": 35,
      "start_time": 84466.0,
      "end_time": 85966.0,
      "text": "I THINK IT&#39;S SURPRISED--"
    },
    {
      "index": 36,
      "start_time": 85966.0,
      "end_time": 87666.0,
      "text": "IT HAD A ROUGH START,"
    },
    {
      "index": 37,
      "start_time": 87666.0,
      "end_time": 90366.0,
      "text": "BUT YOU&#39;VE GOT 5 MILLION PEOPLE THAT HAVE SIGNED UP SO FAR,"
    },
    {
      "index": 38,
      "start_time": 90366.0,
      "end_time": 95633.0,
      "text": "WHICH IS AN ENORMOUS AMOUNT OF PEOPLE THAT HAVE SIGNED UP,"
    },
    {
      "index": 39,
      "start_time": 95633.0,
      "end_time": 97166.0,
      "text": "SO IT&#39;S SUCCESSFUL."
