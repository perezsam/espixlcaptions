    {
      "index": 80,
      "start_time": 268700.0,
      "end_time": 269783.0,
      "text": "back to here?"
    },
    {
      "index": 81,
      "start_time": 271541.0,
      "end_time": 272874.0,
      "text": "Fusion slap-off."
