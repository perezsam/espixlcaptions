    },
    {
      "index": 81,
      "start_time": 469120.0,
      "end_time": 473880.0,
      "text": "Touch me"
    },
    {
      "index": 82,
      "start_time": 475840.0,
      "end_time": 481440.0,
      "text": "like I&#39;ve never loved before,"
    },
    {
      "index": 83,
      "start_time": 482800.0,
      "end_time": 487200.0,
      "text": "in the place that I adore,"
    },
    {
      "index": 84,
      "start_time": 488400.0,
      "end_time": 492880.0,
      "text": "in my hideaway."
    },
    {
      "index": 85,
      "start_time": 496400.0,
      "end_time": 501400.0,
      "text": "I know"
    },
    {
      "index": 86,
      "start_time": 502120.0,
      "end_time": 508280.0,
      "text": "whichever way the wind may blow,"
    },
    {
      "index": 87,
      "start_time": 509560.0,
      "end_time": 513640.0,
      "text": "there will be a place for me to go"
    },
    {
      "index": 88,
      "start_time": 515440.00000000006,
      "end_time": 520240.00000000006,
