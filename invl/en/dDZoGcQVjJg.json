    },
    {
      "index": 89,
      "start_time": 521400.0,
      "end_time": 523360.0,
      "text": "My hideaway."
    },
    {
      "index": 90,
      "start_time": 582400.0,
      "end_time": 586896.0,
      "text": "Sticks and stones"
    },
    {
      "index": 91,
      "start_time": 586920.0,
      "end_time": 589560.0,
      "text": "I won&#39;t hide from you no more."
    },
    {
      "index": 92,
      "start_time": 593800.0,
      "end_time": 597376.0,
      "text": "And in time"
    },
    {
      "index": 93,
      "start_time": 597400.0,
      "end_time": 599959.0,
      "text": "I find what I&#39;ve been searching for."
    },
    {
      "index": 94,
      "start_time": 603480.0,
      "end_time": 608336.0,
      "text": "Heard your voice"
    },
    {
      "index": 95,
      "start_time": 608360.0,
