    },
    {
      "index": 240,
      "start_time": 753710.0,
      "end_time": 756600.0,
      "text": "It&#39;s got a little bit of flexibility"
    },
    {
      "index": 241,
      "start_time": 756600.0,
      "end_time": 758170.0,
      "text": "it&#39;s very, very rigid."
    },
    {
      "index": 242,
      "start_time": 758170.0,
      "end_time": 759579.0,
      "text": "That hollow sound."
    },
    {
      "index": 243,
      "start_time": 759580.0,
      "end_time": 764270.0,
      "text": "A minus 150 (plink sound)"
    },
    {
      "index": 244,
      "start_time": 766800.0,
      "end_time": 770930.0,
      "text": "A plus 150 (plink sound)"
    },
    {
      "index": 245,
      "start_time": 773560.0,
      "end_time": 778560.0,
