    {
      "index": 128,
      "start_time": 465980.0,
      "end_time": 468250.0,
      "text": "You know, Eric is smiling."
    },
    {
      "index": 129,
      "start_time": 468250.0,
      "end_time": 472340.0,
      "text": "You know, great minds think alike."
    },
    {
      "index": 130,
      "start_time": 472340.0,
