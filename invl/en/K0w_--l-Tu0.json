    {
      "index": 109,
      "start_time": 295196.0,
      "end_time": 297290.0,
      "text": "- Okay, one more time."
    },
    {
      "index": 110,
      "start_time": 308221.0,
      "end_time": 309462.0,
      "text": "- Is she okay?"
    },
    {
      "index": 111,
      "start_time": 309462.0,
      "end_time": 312578.0,
