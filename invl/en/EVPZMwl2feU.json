    {
      "index": 24,
      "start_time": 52840.0,
      "end_time": 55644.0,
      "text": "(playful music)"
    },
    {
      "index": 25,
      "start_time": 66606.0,
      "end_time": 68644.0,
      "text": "- [Lincoln] Full speed ahead."
    },
    {
      "index": 26,
      "start_time": 72039.0,
      "end_time": 73616.0,
      "text": "(laughing)"
    },
    {
      "index": 27,
      "start_time": 82809.0,
      "end_time": 84120.0,
      "text": "Look, a train!"
    },
    {
      "index": 28,
      "start_time": 84120.0,
      "end_time": 85516.0,
      "text": "- [All] A train!"
    },
    {
      "index": 29,
      "start_time": 85516.0,
      "end_time": 87647.0,
      "text": "- That was a lot of fun, we\nrode it all over the place."
    },
    {
      "index": 30,
      "start_time": 87655.0,
      "end_time": 89903.0,
      "text": "It was a blast, we kind of\ndamaged it a little bit."
    },
    {
      "index": 31,
      "start_time": 89903.0,
      "end_time": 92853.0,
      "text": "It's kind of beat up a little\nbit, because we rolled it"
