    {
      "index": 52,
      "start_time": 140818.0,
      "end_time": 142223.0,
      "text": "I don't want to mess with it too much."
    },
    {
      "index": 53,
      "start_time": 142223.0,
      "end_time": 143165.0,
      "text": "I'm gonna take a screwdriver"
    },
    {
      "index": 54,
      "start_time": 143165.0,
      "end_time": 146280.0,
