    },
    {
      "index": 12,
      "start_time": 51400.0,
      "end_time": 53400.0,
      "text": "Wingardium Leviosa."
    },
    {
      "index": 13,
      "start_time": 58480.0,
      "end_time": 60980.0,
      "text": "Look at you playing with your cards."
    },
    {
      "index": 14,
      "start_time": 61120.0,
      "end_time": 64340.0,
      "text": "Pathetic! We've got final exams coming up soon."
    },
    {
      "index": 15,
      "start_time": 64900.00000000001,
