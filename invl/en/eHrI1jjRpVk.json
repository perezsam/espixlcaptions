    {
      "index": 12,
      "start_time": 22570.0,
      "end_time": 27100.0,
      "text": "Stacy asks Dear Blocko, can a dead person come back to life?"
    },
    {
      "index": 13,
      "start_time": 27100.0,
      "end_time": 28310.0,
      "text": "I LOVE you, Blocko!﻿"
    },
    {
      "index": 14,
      "start_time": 28310.0,
      "end_time": 29310.0,
      "text": "Hey, thanks!"
    },
    {
      "index": 15,
      "start_time": 29310.0,
