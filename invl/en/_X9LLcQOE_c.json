  "json": [
    {
      "index": 1,
      "start_time": 1890.0,
      "end_time": 4399.0,
      "text": "Antonio: Everybody's talking about foldable smartphones."
    },
    {
      "index": 2,
      "start_time": 4400.0,
      "end_time": 7900.0,
      "text": "This is fresh. This is new. This shows promise"
    },
    {
      "index": 3,
      "start_time": 7900.0,
