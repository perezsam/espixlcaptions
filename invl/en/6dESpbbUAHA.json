    {
      "index": 63,
      "start_time": 226740.0,
      "end_time": 229640.0,
      "text": "Okay so let's test these out."
    },
    {
      "index": 64,
      "start_time": 230220.0,
      "end_time": 232620.0,
      "text": "Her daughter is intelligent."
