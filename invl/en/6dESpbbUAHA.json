    {
      "index": 26,
      "start_time": 90820.0,
      "end_time": 92540.0,
      "text": "Let me show you what I mean."
    },
    {
      "index": 27,
      "start_time": 93840.0,
      "end_time": 95940.0,
      "text": "That shop assistant was rude."
