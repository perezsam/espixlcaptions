    {
      "index": 124,
      "start_time": 446380.0,
      "end_time": 448440.0,
      "text": "Your resume is quite impressive."
    },
    {
      "index": 125,
      "start_time": 449700.0,
      "end_time": 451380.0,
      "text": "Those desserts are good."
    },
    {
      "index": 126,
      "start_time": 452800.0,
      "end_time": 455780.0,
      "text": "Those desserts are particularly good."
    },
    {
      "index": 127,
      "start_time": 456820.0,
      "end_time": 458620.0,
      "text": "The movie was okay but it got"
