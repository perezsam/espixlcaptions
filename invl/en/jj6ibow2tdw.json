    {
      "index": 95,
      "start_time": 288210.0,
      "end_time": 289422.0,
      "text": "IT&#39;S NOT UPDATED."
    },
    {
      "index": 96,
      "start_time": 289422.0,
      "end_time": 290623.0,
      "text": "WHAT WAS MY POINT?"
    },
    {
      "index": 97,
      "start_time": 290623.0,
      "end_time": 292925.0,
      "text": "THE TURTLE FIX."
