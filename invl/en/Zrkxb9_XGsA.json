    {
      "index": 16,
      "start_time": 45505.0,
      "end_time": 47463.0,
      "text": "Thanks man, big fan of yours too."
    },
    {
      "index": 17,
      "start_time": 47803.0,
      "end_time": 48223.0,
      "text": "Yeah."
    },
    {
      "index": 18,
      "start_time": 48223.0,
      "end_time": 49694.0,
      "text": "You make me feel like dancing."
