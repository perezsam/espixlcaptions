    {
      "index": 47,
      "start_time": 133400.0,
      "end_time": 134400.0,
      "text": "Feifei: Rob?"
    },
    {
      "index": 48,
      "start_time": 134400.0,
      "end_time": 135720.0,
      "text": "Rob: Ah. Am I waffling on?"
