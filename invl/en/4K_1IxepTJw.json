    {
      "index": 105,
      "start_time": 216820.0,
      "end_time": 218120.0,
      "text": "Sign me up please!"
    },
    {
      "index": 106,
      "start_time": 218120.0,
      "end_time": 220160.0,
      "text": "Bakery Bar is the storefront of Debbie Does Doberge"
    },
    {
      "index": 107,
      "start_time": 220160.0,
