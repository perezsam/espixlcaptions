    {
      "index": 40,
      "start_time": 295141.0,
      "end_time": 297858.0,
      "text": "Your fellowship is very practical."
    },
    {
      "index": 41,
      "start_time": 297858.0,
      "end_time": 299490.0,
      "text": "Yes!"
    },
    {
      "index": 42,
      "start_time": 299500.0,
      "end_time": 300162.0,
      "text": "Now I understand that"
