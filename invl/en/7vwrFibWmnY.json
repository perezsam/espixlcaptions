    },
    {
      "index": 190,
      "start_time": 342830.0,
      "end_time": 345619.0,
      "text": "[LAUGHS] I cannot wait to see their reaction."
    },
    {
      "index": 191,
      "start_time": 345619.0,
      "end_time": 346535.0,
      "text": "You know that anxiety?"
    },
    {
      "index": 192,
      "start_time": 346535.0,
      "end_time": 347350.0,
      "text": "I&#39;m nervous."
    },
    {
      "index": 193,
      "start_time": 347350.0,
      "end_time": 348800.0,
      "text": "I&#39;m feeling it inside."
    },
    {
      "index": 194,
      "start_time": 348800.0,
      "end_time": 349610.0,
      "text": "Open your eyes."
    },
    {
      "index": 195,
      "start_time": 349610.0,
