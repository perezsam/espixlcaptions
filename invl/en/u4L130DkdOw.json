  "json": [
    {
      "index": 1,
      "start_time": 13309.0,
      "end_time": 16221.0,
      "text": "What images do we see from the rest of the world?"
    },
    {
      "index": 2,
      "start_time": 16714.0,
      "end_time": 19113.0,
      "text": "We see natural disasters,"
