    },
    {
      "index": 40,
      "start_time": 99849.0,
      "end_time": 101659.0,
      "text": "You're embarrassing yourself."
    },
    {
      "index": 41,
      "start_time": 101659.0,
      "end_time": 103919.0,
      "text": "- That's what I mean. - He's embarrassing us."
    },
    {
      "index": 42,
      "start_time": 104239.0,
      "end_time": 106330.0,
      "text": "It's not about you, lady."
    },
    {
      "index": 43,
      "start_time": 106330.0,
