      "end_time": 862160.0,
      "text": "turn the oven off"
    },
    {
      "index": 203,
      "start_time": 862160.0,
      "end_time": 864160.0,
      "text": "take a wood spoon"
    },
    {
      "index": 204,
      "start_time": 864160.0,
      "end_time": 866160.0,
      "text": "for keep door a gap"
    },
    {
      "index": 205,
      "start_time": 866160.0,
      "end_time": 868160.0,
      "text": "let the door hold the spoon"
    },
