    {
      "index": 62,
      "start_time": 101302.0,
      "end_time": 102222.0,
      "text": "Once the weapons stop,"
    },
    {
      "index": 63,
      "start_time": 102222.0,
      "end_time": 103562.0,
      "text": "Israel has no choice but to speak,"
