    {
      "index": 83,
      "start_time": 240260.0,
      "end_time": 241940.0,
      "text": "internalize the patriarchy."
    },
    {
      "index": 84,
      "start_time": 242820.0,
      "end_time": 244356.0,
      "text": "This is really critical."
    },
    {
      "index": 85,
      "start_time": 244380.0,
      "end_time": 247156.0,
      "text": "Patriarchy is not boys and men."
