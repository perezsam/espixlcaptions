    {
      "index": 123,
      "start_time": 231430.0,
      "end_time": 232920.0,
      "text": "does that sound like a place you wanna work?"
    },
    {
      "index": 124,
      "start_time": 232920.0,
      "end_time": 234730.0,
      "text": "Does that sound like a place with work life balance"
