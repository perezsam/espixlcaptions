    },
    {
      "index": 20,
      "start_time": 38010.0,
      "end_time": 38860.0,
      "text": "What's your name?"
    },
    {
      "index": 21,
      "start_time": 38860.0,
      "end_time": 39693.0,
      "text": "- Brayden."
    },
    {
      "index": 22,
      "start_time": 39693.0,
