    {
      "index": 123,
      "start_time": 483300.0,
      "end_time": 484580.0,
      "text": "Look at that."
    },
    {
      "index": 124,
      "start_time": 484580.0,
      "end_time": 485580.0,
      "text": "It's awesome!"
    },
    {
      "index": 125,
      "start_time": 485580.0,
      "end_time": 486580.0,
      "text": "Mmmmm!"
    },
    {
      "index": 126,
      "start_time": 486580.0,
      "end_time": 488400.0,
      "text": "That like melted in my mouth."
    },
    {
      "index": 127,
      "start_time": 488400.0,
      "end_time": 489400.0,
      "text": "Wow!"
    },
    {
      "index": 128,
      "start_time": 489400.0,
      "end_time": 490530.0,
      "text": "That was really good."
    },
    {
      "index": 129,
      "start_time": 490530.0,
      "end_time": 493320.0,
      "text": "Here's the other half."
    },
    {
      "index": 130,
      "start_time": 493320.0,
      "end_time": 498169.0,
      "text": "Mmmm\nI could eat this everyday."
    },
    {
      "index": 131,
      "start_time": 498169.0,
      "end_time": 500590.0,
      "text": "It's so good."
    },
    {
      "index": 132,
      "start_time": 500590.0,
      "end_time": 508830.0,
      "text": "So we just crushed that fish dish, it was\ndelicious."
    },
