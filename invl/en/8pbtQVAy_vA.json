    {
      "index": 49,
      "start_time": 219849.0,
      "end_time": 220849.0,
      "text": "Guava juice."
    },
    {
      "index": 50,
      "start_time": 220849.0,
      "end_time": 221849.0,
      "text": "Oh, it's guava?"
    },
    {
      "index": 51,
      "start_time": 221849.0,
      "end_time": 222849.0,
      "text": "It's delicious."
    },
    {
      "index": 52,
      "start_time": 222849.0,
