    {
      "index": 75,
      "start_time": 162750.0,
      "end_time": 166120.0,
      "text": "he found himself immediately blocked from doing,"
    },
    {
      "index": 76,
      "start_time": 166120.0,
      "end_time": 166680.0,
      "text": "well,"
    },
    {
      "index": 77,
      "start_time": 166680.0,
      "end_time": 167640.0,
      "text": "anything -"
    },
    {
      "index": 78,
      "start_time": 167640.0,
      "end_time": 170300.0,
      "text": "by officials the next level up."
    },
    {
      "index": 79,
      "start_time": 170300.0,
      "end_time": 171620.0,
      "text": "So five years later,"
    },
    {
      "index": 80,
      "start_time": 171620.0,
      "end_time": 173629.0,
