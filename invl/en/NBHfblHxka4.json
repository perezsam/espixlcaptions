    },
    {
      "index": 4,
      "start_time": 10110.0,
      "end_time": 11710.0,
      "text": "Hello and welcome to China Uncensored,"
    },
    {
      "index": 5,
      "start_time": 11710.0,
      "end_time": 13750.0,
      "text": "I'm your host, Chris Chappell."
    },
    {
      "index": 6,
      "start_time": 13750.0,
