    },
    {
      "index": 98,
      "start_time": 403620.0,
      "end_time": 406380.0,
      "text": "I want to thank you properly through action, Wushuang"
    },
    {
      "index": 99,
      "start_time": 406380.0,
      "end_time": 408580.0,
      "text": "I wanted to tell you this for a while,"
    },
    {
      "index": 100,
      "start_time": 408580.0,
      "end_time": 409920.0,
      "text": "but I never had the chance"
    },
    {
      "index": 101,
      "start_time": 409920.0,
      "end_time": 410820.0,
      "text": "The thing is,"
    },
    {
      "index": 102,
      "start_time": 410820.0,
      "end_time": 412020.0,
      "text": "if you wish"
    },
    {
      "index": 103,
      "start_time": 412020.0,
      "end_time": 414440.0,
      "text": "I want to pass on the Ancient Tomb sect martial art to you"
