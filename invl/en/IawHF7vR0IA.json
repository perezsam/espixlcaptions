    },
    {
      "index": 74,
      "start_time": 176400.0,
      "end_time": 178211.0,
      "text": "What will you have? Chocolate?"
    },
    {
      "index": 75,
      "start_time": 178344.0,
      "end_time": 180813.0,
      "text": "Hm, nah. I think I'll have strawberry."
    },
    {
      "index": 76,
      "start_time": 180947.0,
      "end_time": 182282.0,
      "text": "That's my other favorite flavor."
