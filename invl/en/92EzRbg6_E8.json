    {
      "index": 89,
      "start_time": 243100.0,
      "end_time": 244577.0,
      "text": "What did the Lord Jesus say?"
    },
    {
      "index": 90,
      "start_time": 248315.0,
      "end_time": 249682.0,
      "text": "Yes, right!"
    },
    {
      "index": 91,
      "start_time": 249683.0,
      "end_time": 252986.0,
