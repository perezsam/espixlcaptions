  "json": [
    {
      "index": 1,
      "start_time": 1670.0,
      "end_time": 2402.0,
      "text": "-I´ve got to say I´m a giant fan,"
    },
    {
      "index": 2,
      "start_time": 2402.0,
      "end_time": 4370.0,
      "text": "as you probably heard, that I am a fan of your show."
