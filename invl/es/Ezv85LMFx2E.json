    {
      "index": 72,
      "start_time": 204015.0,
      "end_time": 207000.0,
      "text": "es no empeorar su situación:"
    },
    {
      "index": 73,
      "start_time": 207001.0,
      "end_time": 208010.0,
      "text": "enojándonos..."
