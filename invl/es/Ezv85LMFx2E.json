    {
      "index": 74,
      "start_time": 208011.0,
      "end_time": 209021.0,
      "text": "ignorándolos..."
    },
    {
      "index": 75,
      "start_time": 209022.0,
      "end_time": 212001.0,
      "text": "o burlándonos."
    },
    {
      "index": 76,
      "start_time": 212002.0,
      "end_time": 213010.0,
      "text": "Recuerda,"
    },
    {
      "index": 77,
      "start_time": 213011.0,
      "end_time": 217009.0,
      "text": "sólo porque un juego de XBox no funciona en un Playstation,"
    },
    {
      "index": 78,
      "start_time": 217010.0,
