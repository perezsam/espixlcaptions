    {
      "index": 48,
      "start_time": 116640.0,
      "end_time": 118240.0,
      "text": "y lo nombra profeta"
    },
    {
      "index": 49,
      "start_time": 118400.0,
      "end_time": 123580.0,
      "text": "Ezequiel debe acusar a Israel de romper su acuerdo de pacto con Dios en un par de maneras."
    },
    {
      "index": 50,
      "start_time": 124060.0,
      "end_time": 126400.0,
      "text": "Israel ha dado su lealtad a otros dioses"
    },
    {
      "index": 51,
      "start_time": 126400.0,
      "end_time": 128340.0,
      "text": "y ha estado adorando ídolos"
    },
    {
      "index": 52,
      "start_time": 128340.0,
      "end_time": 129520.0,
      "text": "y todo esto les ha llevado a"
    },
    {
      "index": 53,
      "start_time": 129520.00000000001,
