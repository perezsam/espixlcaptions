    {
      "index": 162,
      "start_time": 305789.0,
      "end_time": 307919.0,
      "text": "desde entonces, y Llivia está separada"
    },
    {
      "index": 163,
      "start_time": 307919.0,
      "end_time": 309690.0,
      "text": "del resto de España por este estrecho"
    },
    {
      "index": 164,
      "start_time": 309690.0,
