    {
      "index": 13,
      "start_time": 43423.0,
      "end_time": 46892.0,
      "text": "hizo algunos bocaditos al estilo coreano\nllamada moo-jang-ah-ti"
    },
    {
      "index": 14,
      "start_time": 46892.0,
      "end_time": 50896.0,
      "text": "Combiné la receta de mi abuela"
