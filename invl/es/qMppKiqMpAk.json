{
  "video_id": "qMppKiqMpAk",
  "title": "Why Earth may someday look like Mars | Anjali Tripathi",
  "json": [
    {
      "index": 1,
      "start_time": 0.0,
      "end_time": 7000.0,
      "text": "Traductor: Sebastian Betti\nRevisor: Lidia Cámara de la Fuente"
    },
    {
      "index": 2,
      "start_time": 12760.0,
      "end_time": 15735.0,
      "text": "Al mirar las estrellas en la noche,"
    },
    {
      "index": 3,
      "start_time": 15760.0,
      "end_time": 17216.0,
      "text": "es increíble lo que se ve."
    },
    {
      "index": 4,
      "start_time": 17240.0,
      "end_time": 18696.0,
      "text": "Es hermoso."
    },
    {
      "index": 5,
      "start_time": 18720.0,
      "end_time": 21816.0,
      "text": "Pero lo más increíble \nes lo que no se puede ver,"
    },
    {
      "index": 6,
      "start_time": 21840.0,
      "end_time": 23216.0,
      "text": "porque lo que sabemos ahora"
    },
    {
      "index": 7,
      "start_time": 23240.0,
