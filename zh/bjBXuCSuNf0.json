    {
      "index": 118,
      "start_time": 684619.0,
      "end_time": 688300.0,
      "text": "Ah ... entonces voy a verle. Vale."
    },
    {
      "index": 119,
      "start_time": 688319.0,
      "end_time": 689720.0,
      "text": "Vale."
