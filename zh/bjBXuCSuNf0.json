    {
      "index": 120,
      "start_time": 689720.0,
      "end_time": 691189.0,
      "text": "Muy bien."
    },
    {
      "index": 121,
      "start_time": 695000.0,
      "end_time": 698189.0,
      "text": "¡Hasta luego!"
    },
    {
      "index": 122,
      "start_time": 699800.0,
      "end_time": 701000.0,
      "text": "Hasta luego."
    },
    {
      "index": 123,
      "start_time": 717000.0,
      "end_time": 719000.0,
      "text": "Hola, Sam."
    },
    {
      "index": 124,
      "start_time": 719000.0,
      "end_time": 728000.0,
      "text": "¡Pablo! ¡Colega! Estás de vuelta. \n¿Cómo te ha ido?"
    },
    {
      "index": 125,
      "start_time": 729100.0,
